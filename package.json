{
	"name": "alpha-vantage-graphql",
	"version": "1.0.0",
	"description": "",
	"main": "src/index.js",
	"directories": {
		"test": "test"
	},
	"keywords": [
		"alpha vantage",
		"vantage",
		"alpha",
		"alphavantage",
		"stocks",
		"stock market",
		"api"
	],
	"author": {
		"name": "Dominik Maszczyk",
		"email": "Skitionek@gmail.com",
		"web": "https://www.linkedin.com/in/dominik-maszczyk/"
	},
	"maintainers": [
		{
			"name": "Dominik Maszczyk <Skitionek@gmail.com> (https://www.linkedin.com/in/dominik-maszczyk/)"
		}
	],
	"contributors": [
		"Zack Urben <zackurben@gmail.com> (http://zackurben.com)"
	],
	"license": "MIT",
	"homepage": "https://github.com/biosustain-gem/alpha-vantage-graphql",
	"bugs": {
		"web": "https://github.com/biosustain-gem/alpha-vantage-graphql/issues"
	},
	"engines": {
		"node": ">=6.0.0"
	},
	"scripts": {
		"start": "nodemon -e js,graphql -r node_modules/dotenv/config --exec babel-node --inspect ./src/index.js",
		"test": "jest",
		"prebuild": "npm run clean",
		"build": "babel src --out-dir ./dist --source-maps --copy-files",
		"clean": "rimraf scripts/dist/",
		"debug": "nodemon -e js,graphql -r node_modules/dotenv/config --exec babel-node --inspect ./src/index.js"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.4",
		"@babel/node": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-decorators": "^7.4.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"@graphql-modules/core": "^0.7.1",
		"@graphql-modules/di": "^0.7.1",
		"apollo-server": "^2.4.8",
		"apollo-server-testing": "^2.4.8",
		"gql-generator-node": "^2.1.4",
		"graphql-import-node": "0.0.4",
		"jest": "^24.7.1",
		"jest-summary-reporter": "0.0.2",
		"jest-transform-graphql": "^2.1.0",
		"nodemon": "^1.19.0",
		"reflect-metadata": "^0.1.13"
	},
	"dependencies": {
		"@babel/polyfill": "^7.4.4",
		"apollo-datasource-rest": "^0.6.1",
		"auto-bind": "^2.1.0",
		"dotenv": "^8.0.0",
		"http-status-codes": "^1.3.2"
	},
	"peerDependencies": {
		"graphql": "^14.2.1"
	},
	"jshintConfig": {
		"node": true,
		"trailingcomma": false,
		"esversion": 6
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		],
		"plugins": [
			"@babel/plugin-proposal-object-rest-spread",
			[
				"@babel/plugin-proposal-decorators",
				{
					"legacy": true
				}
			],
			[
				"@babel/plugin-proposal-class-properties",
				{
					"loose": true
				}
			]
		]
	},
	"eslintConfig": {
		"extends": [
			"eslint-config-prettier",
			"eslint-config-uber-es2015",
			"prettier"
		],
		"env": {
			"es6": true,
			"node": true,
			"jest/globals": true
		},
		"parser": "babel-eslint",
		"plugins": [
			"babel",
			"prettier",
			"jest"
		],
		"rules": {
			"valid-jsdoc": 0,
			"no-var": 0,
			"max-len": 0,
			"prefer-spread": 1,
			"prefer-template": 1,
			"quote-props": 1,
			"spaced-comment": 1,
			"max-params": 0,
			"no-multiple-empty-lines": 1,
			"no-process-env": 0,
			"no-inline-comments": 0,
			"no-invalid-this": 0,
			"no-unused-expressions": 0,
			"camelcase": 0,
			"consistent-return": 0,
			"comma-dangle": 1,
			"no-magic-numbers": [
				"error",
				{
					"ignore": [
						0,
						1,
						2,
						100
					],
					"enforceConst": true
				}
			],
			"indent": [
				"error",
				"tab"
			]
		}
	},
	"jest": {
		"transform": {
			"\\.(gql|graphql)$": "jest-transform-graphql",
			".*": "babel-jest"
		},
		"reporters": [
			"default",
			[
				"jest-summary-reporter",
				{
					"failuresOnly": false
				}
			]
		],
		"moduleFileExtensions": [
			"js",
			"graphql"
		]
	}
}
