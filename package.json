{
	"name": "alpha-vantage-graphql",
	"version": "1.2.9",
	"description": "",
	"main": "lib/index.js",
	"files": [
		"lib",
		"src"
	],
	"directories": {
		"test": "test"
	},
	"keywords": [
		"alpha vantage",
		"vantage",
		"alpha",
		"alphavantage",
		"stocks",
		"stock market",
		"api"
	],
	"author": {
		"name": "Dominik Maszczyk",
		"email": "Skitionek@gmail.com",
		"web": "https://www.linkedin.com/in/dominik-maszczyk/"
	},
	"maintainers": [
		{
			"name": "Dominik Maszczyk <Skitionek@gmail.com> (https://www.linkedin.com/in/dominik-maszczyk/)"
		}
	],
	"contributors": [
		"Zack Urben <zackurben@gmail.com> (http://zackurben.com)"
	],
	"license": "MIT",
	"homepage": "https://github.com/biosustain-gem/alpha-vantage-graphql",
	"bugs": {
		"web": "https://github.com/biosustain-gem/alpha-vantage-graphql/issues"
	},
	"engines": {
		"node": ">=6.0.0"
	},
	"scripts": {
		"start": "nodemon -e js,graphql -r node_modules/dotenv/config --exec babel-node --inspect ./src/index.js",
		"test": "jest",
		"spawnServer": "nodemon -e js,graphql -r node_modules/dotenv/config --exec babel-node --inspect ./test/spawnServer.js",
		"prebuild": "npm run clean",
		"debug": "nodemon -e js,graphql -r node_modules/dotenv/config --exec babel-node --inspect ./src/index.js",
		"semantic-release": "semantic-release",
		"clean": "rimraf lib",
		"test:prod": "cross-env BABEL_ENV=production npm run test",
		"test:only": "jest",
		"test:watch": "npm test -- --watch",
		"test:examples": "node examples/",
		"cover": "jest --coverage",
		"lint": "eslint src test",
		"build": "cross-env BABEL_ENV=production babel src --out-dir lib --source-maps --copy-files",
		"prepublish": "npm run build"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/node": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-decorators": "^7.4.4",
		"@babel/plugin-proposal-export-default-from": "^7.5.2",
		"@babel/plugin-proposal-export-namespace-from": "^7.5.2",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@graphql-modules/core": "^0.7.9",
		"apollo-server": "^2.8.1",
		"apollo-server-testing": "^2.8.1",
		"babel-eslint": "^10.0.2",
		"cross-env": "^5.2.0",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-config-uber-es2015": "^3.1.2",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-jest": "^22.15.1",
		"eslint-plugin-prettier": "^3.1.0",
		"gql-generator-node": "^2.1.4",
		"jest": "^24.8.0",
		"jest-summary-reporter": "0.0.2",
		"jest-transform-graphql": "^2.1.0",
		"jsonic": "^0.3.1",
		"lodash": "latest",
		"lodash.intersection": "^4.4.0",
		"lodash.mapvalues": "^4.6.0",
		"nodemon": "^1.19.1",
		"prettier": "2.0.5",
		"reflect-metadata": "^0.1.13"
	},
	"dependencies": {
		"@babel/polyfill": "^7.4.4",
		"alpha-vantage-data-source": "latest",
		"babel-plugin-import-graphql": "^2.7.0",
		"d3-time-format": "^2.1.3",
		"dotenv": "^8.0.0",
		"graphql-fields": "^2.0.3",
		"lodash.merge": "^4.6.2"
	},
	"peerDependencies": {
		"graphql": "^14.2.1"
	},
	"jshintConfig": {
		"node": true,
		"trailingcomma": false,
		"esversion": 6
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		],
		"plugins": [
			"import-graphql",
			"@babel/plugin-proposal-object-rest-spread",
			[
				"@babel/plugin-proposal-decorators",
				{
					"legacy": true
				}
			],
			[
				"@babel/plugin-proposal-class-properties",
				{
					"loose": true
				}
			],
			"@babel/plugin-proposal-export-namespace-from",
			"@babel/plugin-proposal-export-default-from"
		]
	},
	"eslintConfig": {
		"extends": [
			"eslint-config-prettier",
			"eslint-config-uber-es2015",
			"prettier"
		],
		"env": {
			"es6": true,
			"node": true,
			"jest/globals": true
		},
		"parser": "babel-eslint",
		"plugins": [
			"babel",
			"prettier",
			"jest"
		],
		"rules": {
			"valid-jsdoc": 0,
			"no-var": 0,
			"max-len": 0,
			"prefer-spread": 1,
			"prefer-template": 1,
			"quote-props": 1,
			"spaced-comment": 1,
			"max-params": 0,
			"no-multiple-empty-lines": 1,
			"no-process-env": 0,
			"no-inline-comments": 0,
			"no-invalid-this": 0,
			"no-unused-expressions": 0,
			"camelcase": 0,
			"consistent-return": 0,
			"comma-dangle": 1,
			"no-magic-numbers": [
				"error",
				{
					"ignore": [
						0,
						1,
						2,
						100
					],
					"enforceConst": true
				}
			],
			"indent": [
				"error",
				"tab"
			]
		}
	},
	"jest": {
		"transform": {
			".*": "babel-jest"
		},
		"reporters": [
			"default",
			[
				"jest-summary-reporter",
				{
					"failuresOnly": false
				}
			]
		],
		"moduleFileExtensions": [
			"js",
			"graphql",
			"json"
		]
	}
}
